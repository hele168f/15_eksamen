                fill="#F39C12"></path>
              <path
                d="M61.4128 0L64.1334 8.37293H72.9372L65.8147 13.5477L68.5353 21.9206L61.4128 16.7459L54.2904 21.9206L57.0109 13.5477L49.8885 8.37293H58.6923L61.4128 0Z"
                fill="#F39C12"></path>
              <path
                d="M85.6477 0L88.3682 8.37293H97.1721L90.0496 13.5477L92.7701 21.9206L85.6477 16.7459L78.5253 21.9206L81.2458 13.5477L74.1233 8.37293H82.9272L85.6477 0Z"
                fill="#F39C12"></path>
              <path
                d="M109.883 0L112.603 8.37293H121.407L114.284 13.5477L117.005 21.9206L109.883 16.7459L102.76 21.9206L105.481 13.5477L98.3582 8.37293H107.162L109.883 0Z"
                fill="#F39C12"></path>
            </svg>
          </div>
        </div>
      </section>

      <section class="indslag">
        <div class="pop">
          <img
            src="https://cdn.shopify.com/s/files/1/0544/0926/5329/files/BIRKE_0003_1080x.jpg?v=1665055551"
            width="100%"
            height="100%"
            alt="First Picture"
          />
        </div>
        <div class="pop_2">
          <h2>BIRKE OG JÆGERKORPSET</h2>
          <p>
            Jægersoldat siden 1987 og tidligere ansvarlig for den fysiske
            træning i korpset gennem et årti, har teset BIRKE's produkter
          </p>
          <Button href="/soldat" variant="secondary">LÆS MERE</Button>
        </div>
      </section>

      <Tilmeld />

      <Footer />
