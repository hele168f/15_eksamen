<style>
  #cookie-consent {
    position: fixed;
    bottom: 0;
    width: 100%;
    background-color: #333;
    color: #fff;
    padding: 12px;
  }

  .cookie-wrapper {
    margin-left: auto;
    margin-right: auto;
  }

  .container-cookie {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 16px;
  }

  .text {
    font-size: 0.875rem;
    font-weight: normal;
    line-height: 1.5;
  }

  .text a {
    color: #fff;
    text-decoration: underline;
    margin-left: 4px;
    margin-right: 4px;
  }

  .button-group {
    display: flex;
    align-items: center;
  }

  .deny-button,
  .accept-button {
    font-size: 0.875rem;
    font-weight: medium;
    padding: 4px 8px;
    border-radius: 4px;
    cursor: pointer;
    transition-duration: 0.3s;
  }

  .deny-button {
    color: #fff;
    border: 1px solid #fff;
    background-color: transparent;
    margin-right: 8px;
  }

  .deny-button:hover {
    background-color: #fff;
    color: #333;
  }

  .accept-button {
    color: #333;
    background-color: #fff;
  }

  .accept-button:hover {
    background-color: #ccc;
  }

  .card_komponent_2 {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    grid-gap: 20px;
    justify-items: center;
  }

  @media (max-width: 500px) {
    section {
      padding: 10px;
    }

    .card_komponent_2 {
      grid-template-columns: 1fr;
      grid-gap: 10px;
    }
  }
  section {
    padding: 20px;
  }

  .card_komponent_1 {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    grid-gap: 20px;
    margin-top: var(--spaceCardStackMd);
  }

  .anmeldelse {
    margin-bottom: var(--spacing-xl);
  }
  .reviews {
    margin-bottom: var(--spacing-xl);
  }
  .news {
    margin-bottom: var(--spacing-xl);
  }

  @media (max-width: 500px) {
    section {
      padding: 10px;
    }

    .card_komponent_1 {
      grid-template-columns: 1fr;
      grid-gap: 10px;
    }
  }

  .container_billeder {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
  }

  .picture {
    width: 50%;
    padding: 10px;
    box-sizing: border-box;
    text-align: center;
  }

  .picture img {
    max-width: 100%;
    height: auto;
  }

  .cta-button {
    display: inline-block;
    margin-top: 10px;
    padding: 10px 20px;
    background-color: #000;
    color: #fff;
    text-decoration: none;
  }

  /* Media Query for screens smaller than 768px */
  @media (max-width: 768px) {
    .picture {
      width: 100%;
    }
  }

  .navbar ul.active {
    display: flex;
    flex-direction: column;
    top: 100px;
    position: fixed;
    background-color: black;
    height: 100vh;
    width: 100vh;
    text-decoration: none !important;
    padding-top: 32px;
    z-index: 1;
  }
  h2 {
    font-family: var(--anonymousProText2xlFontFamily);
    font-weight: bold;
    font-size: var(--fontSize-2xl);
  }
  .anmeldelse {
    margin-bottom: 8px;
  }
  .card_komponent {
    margin-top: 32px;
    margin-bottom: 64px;
  }
  .splash {
    margin-bottom: 32px;
  }
  .indslag {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
    padding: 20px;
    gap: var(--spaceCardInlineXl);
    margin-bottom: 64px;
  }

  .pop {
    width: 100%;
    margin-bottom: 20px;
  }
  .pop_2 {
    width: 100%;
    margin-bottom: 20px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    height: 100%;
  }
  .pop_2 h2 {
    margin-bottom: 8px;
  }
  .pop_2 p {
    margin-bottom: 32px;
  }

  img {
    width: 100%;
    height: auto;
  }

  @media (min-width: 900px) {
    .indslag {
      flex-wrap: nowrap;
    }

    .pop {
      width: 50%;
      margin-bottom: 0;
    }

    .pop_2 {
      width: 50%;
      margin-bottom: 0;
    }

    .pop:nth-child(1) {
      order: -1;
    }
    .bottom-right {
      font-size: 30px !important;
    }
  }
  .knap {
    display: flex;
    gap: 16px;
    padding-bottom: var(--spacingLg);
  }

  .container {
    position: relative;
    text-align: center;
    color: white;
  }

  .bottom-right {
    position: absolute;
    bottom: 32px;
    right: 64px;
    font-size: var(--Font-size-Heading-H1);
    font-family: var(--blackOpsOneText6xlFontFamily);
  }

  .container_billeder {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-bottom: 64px;
  }

  .picture {
    position: relative;
    width: 100%;
    overflow: hidden;
  }

  .picture img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  @media (min-width: 900px) {
    .container_billeder {
      flex-wrap: wrap;
    }

    .picture {
      width: 50%; /* Two pictures per row */
    }
  }

  .cta-button {
    position: absolute;
    bottom: 20px;
    left: 50%;
    transform: translateX(-50%);
    padding: 10px 20px;
    background-color: var(--Action-Primary);

    padding-block: 16px;
    color: black;
    border: none;
    border-radius: 2%;
    font-family: var(--blackOpsOneText6xlFontFamily);
    cursor: pointer;
    display: flex;
    justify-content: center;
    align-items: center;
    max-width: fit-content;
    text-decoration: none;
  }
</style>

<script>
  const cookieConsent = document.querySelector("#cookie-consent");
  const cookieAcceptButton = document.querySelector("#cookie-accept");
  const cookieDenyButton = document.querySelector("#cookie-deny");

  cookieAcceptButton.addEventListener("click", () => {
    cookieConsent.remove();
  });

  cookieDenyButton.addEventListener("click", () => {
    cookieConsent.remove();
  });
</script>
